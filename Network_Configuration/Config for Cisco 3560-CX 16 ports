!
version 15.2
no service timestamps log datetime msec
no service timestamps debug datetime msec
no service password-encryption
!
hostname VSK-SW-Office-01
!
spanning-tree mode pvst
spanning-tree extend system-id
!
!
! --- Access Points ---
interface GigabitEthernet0/1
 description AP01
 switchport trunk allowed vlan 10,20,30,40,50
 switchport mode trunk
 spanning-tree portfast trunk
!
interface GigabitEthernet0/2
 description AP02
 switchport trunk allowed vlan 10,20,30,40,50
 switchport mode trunk
 spanning-tree portfast trunk
!
! --- Staff VLAN 20 ---
interface GigabitEthernet0/3
 description Staff-PC-01
 switchport access vlan 20
 switchport mode access
 spanning-tree portfast
!
interface GigabitEthernet0/4
 description Staff-PC-02
 switchport access vlan 20
 switchport mode access
 spanning-tree portfast
!
interface GigabitEthernet0/5
 description Staff-PC-03
 switchport access vlan 20
 switchport mode access
 spanning-tree portfast
!
interface GigabitEthernet0/6
 description Staff-PC-04
 switchport access vlan 20
 switchport mode access
 spanning-tree portfast
!
! --- Router Uplink ---
interface GigabitEthernet0/13
 description Uplink-to-Router-Gi0/0
 switchport trunk allowed vlan 10,20,30,40,50
 switchport mode trunk
!
! --- Admin VLAN 50 ---
interface GigabitEthernet0/14
 description Admin-Port
 switchport access vlan 50
 switchport mode access
 spanning-tree portfast
!
! --- Other Ports Shutdown by Default ---
interface range GigabitEthernet0/7 - 0/12 , GigabitEthernet0/15 - 0/16
 shutdown
!
vlan 10
 name Student
 exit

vlan 20
 name Staff
 exit

vlan 30
 name Management
 exit

vlan 40
 name Guest
 exit

vlan 50
 name Admin
 exit


interface Vlan1
 no ip address
 shutdown
!
